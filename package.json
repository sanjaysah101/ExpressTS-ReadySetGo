{
  "name": "expressts-readysetgo",
  "version": "1.0.0",
  "description": "An Express.js and TypeScript boilerplate equipped with ESLint, Prettier, Husky, Jest, and Supertest, ready to kickstart your backend development.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sknepali/ExpressTS-ReadySetGo.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "Node",
    "Husky",
    "Jest",
    "Prettier",
    "ESLint",
    "Boilerplate",
    "Backend template",
    "Advance Node Express API Start Up File"
  ],
  "author": "sanjay sah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sknepali/ExpressTS-ReadySetGo.git/issues"
  },
  "homepage": "https://github.com/sknepali/ExpressTS-ReadySetGo.git/blob/main/README.md",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
